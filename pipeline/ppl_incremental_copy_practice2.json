{
	"name": "ppl_incremental_copy_practice2",
	"properties": {
		"activities": [
			{
				"name": "Get Metadata NBN",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "ds_NBN_MD",
						"type": "DatasetReference"
					},
					"fieldList": [
						"childItems"
					],
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "DelimitedTextReadSettings"
					}
				}
			},
			{
				"name": "ForEach_NBN",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Get Metadata NBN",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Get Metadata NBN').output.childItems",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "If Condition_NBN",
							"type": "IfCondition",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"expression": {
									"value": "@greater(activity('Get Metadata NBN').output.lastModified,variables('PreviousNBNFiles'))",
									"type": "Expression"
								},
								"ifTrueActivities": [
									{
										"name": "Set variable_NBN_Filename",
										"type": "SetVariable",
										"dependsOn": [],
										"policy": {
											"secureOutput": false,
											"secureInput": false
										},
										"userProperties": [],
										"typeProperties": {
											"variableName": "CurrentNBNFile",
											"value": {
												"value": "@activity('Get Metadata NBN').output.itemName",
												"type": "Expression"
											}
										}
									},
									{
										"name": "Set variable NBN LastModified",
										"type": "SetVariable",
										"dependsOn": [
											{
												"activity": "Set variable_NBN_Filename",
												"dependencyConditions": [
													"Succeeded"
												]
											}
										],
										"policy": {
											"secureOutput": false,
											"secureInput": false
										},
										"userProperties": [],
										"typeProperties": {
											"variableName": "PreviousNBNFiles",
											"value": {
												"value": "@activity('Get Metadata NBN').output.lastModified",
												"type": "Expression"
											}
										}
									},
									{
										"name": "Copy data_ NBN_Incremental",
										"type": "Copy",
										"dependsOn": [
											{
												"activity": "Set variable NBN LastModified",
												"dependencyConditions": [
													"Succeeded"
												]
											}
										],
										"policy": {
											"timeout": "0.12:00:00",
											"retry": 0,
											"retryIntervalInSeconds": 30,
											"secureOutput": false,
											"secureInput": false
										},
										"userProperties": [],
										"typeProperties": {
											"source": {
												"type": "DelimitedTextSource",
												"storeSettings": {
													"type": "AzureBlobFSReadSettings",
													"recursive": true,
													"enablePartitionDiscovery": false
												},
												"formatSettings": {
													"type": "DelimitedTextReadSettings"
												}
											},
											"sink": {
												"type": "DelimitedTextSink",
												"storeSettings": {
													"type": "AzureBlobFSWriteSettings"
												},
												"formatSettings": {
													"type": "DelimitedTextWriteSettings",
													"quoteAllText": true,
													"fileExtension": ".txt"
												}
											},
											"enableStaging": false,
											"translator": {
												"type": "TabularTranslator",
												"typeConversion": true,
												"typeConversionSettings": {
													"allowDataTruncation": true,
													"treatBooleanAsNumber": false
												}
											}
										},
										"inputs": [
											{
												"referenceName": "DS_NBN_Parameterized",
												"type": "DatasetReference",
												"parameters": {
													"ItenMane(NBN)": {
														"value": "@variables('CurrentNBNFile')",
														"type": "Expression"
													}
												}
											}
										],
										"outputs": [
											{
												"referenceName": "DS_NBN_Sink_p",
												"type": "DatasetReference"
											}
										]
									}
								]
							}
						}
					]
				}
			}
		],
		"variables": {
			"CurrentNBNFile": {
				"type": "String"
			},
			"PreviousNBNFiles": {
				"type": "String"
			}
		},
		"folder": {
			"name": "Incremental_csv_Files"
		},
		"annotations": []
	}
}